<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://images.pexels.com/photos/7234304/pexels-photo-7234304.jpeg?auto=compress&cs=tinysrgb&w=800);">
  <h1>New List</h1>
</div>

<div class="container-md">

  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.submit %>
    <%= f.input :photo, as: :file %>
  <% end %>

  <%= link_to "Back to home", lists_path %>
</div>
